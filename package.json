{
  "name": "skill-inspector",
  "version": "0.2.1",
  "description": "CLI tool to inspect Agent Skills for malicious behaviors",
  "keywords": [
    "agent",
    "skills",
    "security",
    "inspector"
  ],
  "license": "Apache-2.0",
  "author": "User",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "skill-inspector": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "format": "pnpm format:trunk && pnpm format:eslint",
    "format:eslint": "eslint --fix src/**/*.ts",
    "format:trunk": "trunk fmt",
    "lint": "pnpm lint:trunk && pnpm lint:eslint",
    "lint:eslint": "eslint src/**/*.ts",
    "lint:trunk": "trunk check -y",
    "start": "node dist/index.js",
    "test": "vitest run"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.23",
    "@ai-sdk/google": "^3.0.13",
    "@ai-sdk/google-vertex": "^4.0.28",
    "@ai-sdk/groq": "^1.0.0",
    "@ai-sdk/mistral": "^3.0.18",
    "@ai-sdk/openai": "^3.0.18",
    "@clack/prompts": "^0.11.0",
    "@mastra/core": "^1.2.0",
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "dotenv": "^17.2.3",
    "gray-matter": "^4.0.3",
    "isomorphic-git": "^1.37.1",
    "memfs": "^4.56.10",
    "simple-git": "^3.30.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@trunkio/launcher": "^1.3.4",
    "@types/node": "^25.0.10",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.1",
    "vitest": "^4.0.18"
  },
  "packageManager": "pnpm@10.28.1",
  "engines": {
    "node": ">=22.13.0",
    "pnpm": ">=10.28.1"
  },
  "pnpm": {
    "overrides": {
      "@modelcontextprotocol/sdk": "1.26.0",
      "hono": "4.11.7",
      "tar": "^7.5.8"
    }
  }
}
